S_0x5581e61c27a0 .scope module, "shift_left_TB" "shift_left_TB" 2 1;
 .timescale 0 0;
v0x5581e61d5080_0 .var "PData", 7 0;
v0x5581e61d5160_0 .var "PL", 0 0;
v0x5581e61d5230_0 .net "SLData", 7 0, v0x5581e6188490_0;  1 drivers
v0x5581e61d5330_0 .var "clk", 0 0;
S_0x5581e61c2930 .scope module, "DUT_sl" "shift_left" 2 8, 3 1 0, S_0x5581e61c27a0;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "Din";
    .port_info 1 /INPUT 1 "PL";
    .port_info 2 /INPUT 1 "clk";
    .port_info 3 /OUTPUT 8 "D";
v0x5581e6188490_0 .var "D", 7 0;
v0x5581e61d4d60_0 .net "Din", 7 0, v0x5581e61d5080_0;  1 drivers
v0x5581e61d4e40_0 .net "PL", 0 0, v0x5581e61d5160_0;  1 drivers
v0x5581e61d4f10_0 .net "clk", 0 0, v0x5581e61d5330_0;  1 drivers
E_0x5581e61c14e0 .event negedge, v0x5581e61d4e40_0, v0x5581e61d4f10_0;
    .scope S_0x5581e61c2930;
T_0 ;
    %wait E_0x5581e61c14e0;
    %load/vec4 v0x5581e61d4e40_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x5581e61d4d60_0;
    %assign/vec4 v0x5581e6188490_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5581e6188490_0, 4, 5;
    %load/vec4 v0x5581e6188490_0;
    %parti/s 7, 0, 2;
    %ix/load 4, 1, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5581e6188490_0, 4, 5;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5581e61c27a0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x5581e61d5330_0;
    %inv;
    %store/vec4 v0x5581e61d5330_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5581e61c27a0;
T_2 ;
    %vpi_call 2 17 "$dumpfile", "shift_left_TB.vcd" {0 0 0};
    %vpi_call 2 18 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5581e61c27a0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5581e61d5160_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5581e61d5330_0, 0, 1;
    %pushi/vec4 101, 0, 8;
    %store/vec4 v0x5581e61d5080_0, 0, 8;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5581e61d5160_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5581e61d5160_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 24 "$stop" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
