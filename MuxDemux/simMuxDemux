#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55f0c1b9df60 .scope module, "mux_demux_TB" "mux_demux_TB" 2 1;
 .timescale 0 0;
v0x55f0c1bb4530_0 .var "clk_1MHz", 0 0;
v0x55f0c1bb45f0_0 .var "clk_4MHz", 0 0;
v0x55f0c1bb46b0_0 .net "demux_out", 3 0, v0x55f0c1bb3da0_0;  1 drivers
v0x55f0c1bb47b0_0 .net "mux_out", 0 0, v0x55f0c1bb4330_0;  1 drivers
v0x55f0c1bb48a0_0 .var "sel1", 0 0;
v0x55f0c1bb4990_0 .var "sel2", 1 0;
L_0x55f0c1bb4a30 .concat [ 1 1 0 0], v0x55f0c1bb45f0_0, v0x55f0c1bb4530_0;
S_0x55f0c1b9e0f0 .scope module, "demux_inst" "demux_1_to_4" 2 17, 3 1 0, S_0x55f0c1b9df60;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "I";
    .port_info 1 /INPUT 2 "sel";
    .port_info 2 /OUTPUT 4 "Y";
v0x55f0c1b9b4e0_0 .net "I", 0 0, v0x55f0c1bb4330_0;  alias, 1 drivers
v0x55f0c1bb3da0_0 .var "Y", 3 0;
v0x55f0c1bb3e80_0 .net "sel", 1 0, v0x55f0c1bb4990_0;  1 drivers
E_0x55f0c1ba0a20 .event anyedge, v0x55f0c1b9b4e0_0, v0x55f0c1bb3e80_0;
S_0x55f0c1bb3fc0 .scope module, "mux_inst" "mux_2_to_1" 2 10, 4 1 0, S_0x55f0c1b9df60;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "I";
    .port_info 1 /INPUT 1 "sel";
    .port_info 2 /OUTPUT 1 "Y";
v0x55f0c1bb4230_0 .net "I", 1 0, L_0x55f0c1bb4a30;  1 drivers
v0x55f0c1bb4330_0 .var "Y", 0 0;
v0x55f0c1bb4420_0 .net "sel", 0 0, v0x55f0c1bb48a0_0;  1 drivers
E_0x55f0c1ba0610 .event anyedge, v0x55f0c1bb4230_0, v0x55f0c1bb4420_0;
    .scope S_0x55f0c1bb3fc0;
T_0 ;
    %wait E_0x55f0c1ba0610;
    %load/vec4 v0x55f0c1bb4420_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x55f0c1bb4230_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x55f0c1bb4330_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55f0c1bb4230_0;
    %parti/s 1, 1, 2;
    %store/vec4 v0x55f0c1bb4330_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55f0c1b9e0f0;
T_1 ;
    %wait E_0x55f0c1ba0a20;
    %load/vec4 v0x55f0c1bb3e80_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %jmp T_1.4;
T_1.0 ;
    %pushi/vec4 0, 0, 3;
    %load/vec4 v0x55f0c1b9b4e0_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x55f0c1bb3da0_0, 0, 4;
    %jmp T_1.4;
T_1.1 ;
    %pushi/vec4 0, 0, 2;
    %load/vec4 v0x55f0c1b9b4e0_0;
    %concat/vec4; draw_concat_vec4
    %concati/vec4 0, 0, 1;
    %store/vec4 v0x55f0c1bb3da0_0, 0, 4;
    %jmp T_1.4;
T_1.2 ;
    %pushi/vec4 0, 0, 1;
    %load/vec4 v0x55f0c1b9b4e0_0;
    %concat/vec4; draw_concat_vec4
    %concati/vec4 0, 0, 2;
    %store/vec4 v0x55f0c1bb3da0_0, 0, 4;
    %jmp T_1.4;
T_1.3 ;
    %load/vec4 v0x55f0c1b9b4e0_0;
    %concati/vec4 0, 0, 3;
    %store/vec4 v0x55f0c1bb3da0_0, 0, 4;
    %jmp T_1.4;
T_1.4 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x55f0c1b9df60;
T_2 ;
    %delay 500, 0;
    %load/vec4 v0x55f0c1bb4530_0;
    %inv;
    %store/vec4 v0x55f0c1bb4530_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55f0c1b9df60;
T_3 ;
    %delay 125, 0;
    %load/vec4 v0x55f0c1bb45f0_0;
    %inv;
    %store/vec4 v0x55f0c1bb45f0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55f0c1b9df60;
T_4 ;
    %vpi_call 2 29 "$dumpfile", "mux_demux_TB.vcd" {0 0 0};
    %vpi_call 2 30 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55f0c1b9df60 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0c1bb4530_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0c1bb45f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0c1bb48a0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55f0c1bb4990_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55f0c1bb4990_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f0c1bb48a0_0, 0, 1;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x55f0c1bb4990_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x55f0c1bb4990_0, 0, 2;
    %delay 10000, 0;
    %vpi_call 2 44 "$stop" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "mux_demux_TB.v";
