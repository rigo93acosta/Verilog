#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55e17d72d5c0 .scope module, "display" "display" 2 1;
 .timescale 0 0;
v0x55e17d72d750_0 .var "a", 7 0;
v0x55e17d776fe0_0 .var "b", 7 0;
    .scope S_0x55e17d72d5c0;
T_0 ;
    %vpi_call 2 6 "$monitor", "El valor de a es %d", v0x55e17d72d750_0 {0 0 0};
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x55e17d72d750_0, 0, 8;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x55e17d776fe0_0, 0, 8;
    %delay 10, 0;
    %vpi_call 2 8 "$display", "El valor de b es %d", v0x55e17d776fe0_0 {0 0 0};
    %pushi/vec4 20, 0, 8;
    %store/vec4 v0x55e17d72d750_0, 0, 8;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x55e17d776fe0_0, 0, 8;
    %delay 10, 0;
    %vpi_call 2 10 "$display", "El valor de b es %d", v0x55e17d776fe0_0 {0 0 0};
    %pushi/vec4 30, 0, 8;
    %store/vec4 v0x55e17d72d750_0, 0, 8;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x55e17d776fe0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 40, 0, 8;
    %store/vec4 v0x55e17d72d750_0, 0, 8;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x55e17d776fe0_0, 0, 8;
    %delay 10, 0;
    %vpi_call 2 13 "$display", "El valor de b es %d", v0x55e17d776fe0_0 {0 0 0};
    %pushi/vec4 50, 0, 8;
    %store/vec4 v0x55e17d72d750_0, 0, 8;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x55e17d776fe0_0, 0, 8;
    %delay 10, 0;
    %vpi_call 2 15 "$stop" {0 0 0};
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
